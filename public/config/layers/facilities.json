    "geo-column": "gps_beginning",
    

    "detail-view": {
        "UID": "name_of_facility_visited",
        "title": {
            "prop": "name_of_facility_visited",
            "mustache": "{{{name_of_facility_visited}}}"
        },
        "sub-title": {
            "prop": "",
            "mustache": ""
        },
        "basic-info": [
            {
                "value": {
                    "prop": [
                        "Longitude",
                        "Latitude"
                    ],
                    "join": ", "
                },
                "icon": "screenshot",
