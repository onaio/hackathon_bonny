{
    "label": "Health facilities",
    "source": {
        "type": "geojson",
        "featureType": "Point",
        "data": [
           808
        ],
        "join": [
            "name_of_facility_visited",
            "name_of_facility_visited"
           
        ],
        "minzoom": 2,
        "relation": {
            "type": "one-to-one",
            "key": [
                "one",
                "one"
            ]
    }
    },
    "type": "symbol",
    "paint": {
        "text-color": "#0000ff",
        "text-halo-color": "#fff",
        "text-halo-width": 1.3,
        "text-halo-blur": 1
    },
    "layout": {
        "text-size": 10,
        "text-offset": [
            0,
            2
        ],
        "icon-image": "suitcase-15",
        "icon-allow-overlap": true
    },
    "categories": {
        "breaks": "no",
        "label": [
            "Healthy facility"
        ],
        "type": [
            "Health facility"
        ],
        "color": [
            "#000000"
        ]
    },
    "geo-column": "gps_beginning",

    "aggregate": {
        "filter": [
            "states",
            "districts",
            "type_of_facility_visited",
            "ist"
            ],
        "accepted-filter-values": [
            "all",
            "all",
            "all",
            "all"
        ],
        "filter-label": [
             "COUNTY",
             "DISTRICT",
             "FACILITY TYPE",
             "IST"
        ],
        "sub-filter": [
            "",
            "",
            "",
            ""
        ],
        "accepted-sub-filter-values": [
            "",
            "",
            "",
            ""
        ]
    },

    "detail-view": {
        "UID": "name_of_facility_visited",
        "title": {
            "prop": "name_of_facility_visited",
            "mustache": "{{{name_of_facility_visited}}}"
        },
        "sub-title": {
            "prop": "",
            "mustache": ""
        },
        "basic-info": [
            {
                "value": {
                    "prop": [
                        "Longitude",
                        "Latitude"
                    ],
                    "join": ", "
                },
                "icon": "screenshot",
                "alt": "GPS coordinates"
            },
            {
                "value": "date_of_visit",
                "icon": "pushpin",
                "alt": "Date of Visit",
                "useAltAsPrefix": true
            },
            {
                "value": "states",
                "icon": {
                    "FA": "bookmark"
                },
                "alt": "County",
                "useAltAsPrefix": true
            },
            {
                "value": "ist",
                "icon": {
                    "FA": "bookmark"
                },
                "alt": "IST",
                "useAltAsPrefix": true

            },
            {
                "value": "Districts",
                "icon": {
                    "FA": "bookmark"
                },
                "alt": "District",
                "useAltAsPrefix": true
            },
            {
                "value": "wards",
                "icon": {
                    "FA": "bookmark"
                },
                "alt": "Name of Health Area",
                "useAltAsPrefix": true
            },
            {
                "value": "type_of_facility_visited",
                "icon": "pushpin",
                "alt": "Facility Type",
                "useAltAsPrefix": true
            },
            {
                "value": "supervision_type",
                "icon": "pushpin",
                "alt": "Supervision type",
                "useAltAsPrefix": true
            },
            {
                "value": "priority_level",
                "icon": "pushpin",
                "alt": "Priority Level",
                "useAltAsPrefix": true
            },
            {
                "value": "supervised_by_dsno",
                "icon": "pushpin",
                "alt": "District staff last supervision",
                "useAltAsPrefix": true
            },
            {
                "value": "supervised_by_who",
                "icon": "pushpin",
                "alt": "WHO staff last supervision",
                "useAltAsPrefix": true
            }
        ]
    },
    "popup": {
        "join": [
            "name_of_facility_visited",
            "name_of_facility_visited"

        ],
        "body": "{{name_of_facility_visited}}"
    },
    
    "visible": false,
    "credit": "Zoom to see more health facilities",
    "nondefault": true,
    "category": "Indicator"
