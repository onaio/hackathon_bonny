            },
            {
                "value": "community_location/posto",
                "icon": "home",
                "alt": "Posto"
            },
            {
                "value": "community_location/localidade",
                "icon": {
                    "FA": "building"
                },
                "alt": "Localidade"
            },
            {
                "value": "community_details/qn8",
                "icon": {
                    "FA": "layer-group"
                },
                "alt": "Número de famílias existentes"
            }
        ]
